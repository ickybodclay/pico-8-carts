pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

-- array of bullets
bullet = {}
t=0
can_shoot = true

function _init()
  ship = {}
  ship.x = 128/2
  ship.y = 128-16
  ship.spr = 1
  ship.speed = 1
end

function _draw()
  cls(5)
  draw_actor(ship)
  foreach(bullet, draw_actor)
  print("t = " .. t, 32, 10, 6)
end

function draw_actor(actor)
  spr(actor.spr, actor.x, actor.y)
end

function _update()
  control_ship()
  foreach(bullet, update_bullet)
  if(not can_shoot and (t%4) == 0) can_shoot=true

  t += 1
end

function control_ship()
  if(btn(0)) ship.x -= ship.speed --left
  if(btn(1)) ship.x += ship.speed --right
  if(btn(5)) shoot() --x
  --if(btn(2)) ship.y -= ship.speed
  --if(btn(3)) ship.y += ship.speed
end

function update_bullet(b)
  b.y -= b.speed

  if(b.y < 0) then
    del(bullet, b)
  end
end

function shoot()
  if (not can_shoot) return

  local b = {}
  b.x = ship.x
  b.y = ship.y
  b.speed = 5
  b.spr = 2
  add(bullet, b)
  can_shoot = false
end

