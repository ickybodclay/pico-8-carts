pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- pants --

actor = {}

function _init()
  player = {}
  player.x = 1
  player.y = 1
  player.dx = 0
  player.dy = 0
  player.spr = 1
  player.frame = 0
  player.frames = 3
  add(actor, player)
end

function _draw()
  cls(6)

  foreach(actor,draw_actor)
end

function draw_actor(a)
 spr(a.spr + a.frame, a.x, a.y)
end

function _update()
  control_player(player)
end

function control_player(pl)
  local accel = 1
  if (btn(0)) pl.dx -= accel
  if (btn(1)) pl.dx += accel
  if (btn(2)) pl.dy -= accel
  if (btn(3)) pl.dy += accel

  pl.x += pl.dx
  pl.y += pl.dy

  pl.frame += abs(pl.dx)
  pl.frame += abs(pl.dy)
  pl.frame %= pl.frames

  pl.dx = 0
  pl.dy = 0
end


